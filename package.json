{
  "name": "arclib",
  "version": "0.1.0",
  "license": "MIT",
  "author": "erik aronesty",
  "main": "dist/index.js",
  "module": "dist/arclib.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "dts build",
    "lint": "dts lint",
    "prepare": "dts build",
    "size": "size-limit",
    "start": "dts watch",
    "test": "dts test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=12"
  },
  "size-limit": [
    {
      "path": "dist/arclib.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/arclib.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@size-limit/preset-small-lib": "^8.2.4",
    "@tsconfig/recommended": "^1.0.2",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.1.1",
    "babel-jest": "^29.5.0",
    "dts-cli": "^2.0.3",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-watch-typeahead": "^2.2.2",
    "size-limit": "^8.2.4",
    "ts-jest": "^29.1.0",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4"
  }
}
